 <div class="page">
 {{> header}}
 {{> receptionist-sidebar}}
  <div class="content" style="padding:50px ; font-family: 'Times New Roman', Times, serif;">
   <div class="container-fluid p-5 m-2">
  <div class="col-md-12">
        <nav class="navbar">
            <div class="container-fluid mt-3">
                <form class="d-flex" role="search">
                    <input class="form-control shadow-none me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-success mx-3" type="submit">Search</button>
                </form>
            </div>
        </nav>
    </div>
    <hr>
      <table class="table  table-striped table-bordered">
            <thead>
              <tr>
                <th scope="col">User id</th>
                <th scope="col">Full Name</th>
                <th scope="col">Age</th>
                <th scope="col">Sex</th>
                <th scope="col">user name</th>
                <th scope="col">City</th>
                <th scope="col">phone No</th>
                <th scope="col">department</th>
                <th scope="col">specialization</th>
                <th scope="col">role</th>
                <th scope="col" colspan="2">options</th>
              </tr>
            </thead>
            <tbody>
                {{#each user}}
              <tr>
                <th >{{user_id}}</th>
                <td>{{first_name}} {{last_name}}</td>
                <td> {{Age}}</td>
                <td>{{Sex}}</td>
                <td>{{user_name}}</td>
                <td>{{City}}</td>
                <td>{{Phone_no}}</td>
                <td>{{department}}</td>
                <td>{{specialization}}</td>
                <td>{{role}}</td>
                <td> <a class ="btn btn-outline-success btn-sm" href="#">Modify</a> </td>
                <td> <a class ="btn btn-outline-danger btn-sm" href="#">Delete</a> </td>
              </tr>
         {{else}}
          <p>No gigs available</p>
          {{/each}}
            </tbody>
          </table>
        
   </div>
    {{> footer}}
    {{> copy}} 
 </div>
</div>


